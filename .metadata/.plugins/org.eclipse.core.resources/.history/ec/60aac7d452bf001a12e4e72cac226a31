package com.example.vmwarenumbergenerator.model;

import org.springframework.http.HttpStatus;
import org.springframework.http.ResponseEntity;
import org.springframework.util.MultiValueMap;

public class ResponseObject extends ResponseEntity {
	
	private int task;
	
	

	@Override
	public String toString() {
		return "ResponseObject [task=" + task + "]";
	}



	public ResponseObject(HttpStatus status) {
		super(status);
		// TODO Auto-generated constructor stub
	}



	public ResponseObject(MultiValueMap headers, HttpStatus status) {
		super(headers, status);
		// TODO Auto-generated constructor stub
	}



	public ResponseObject(Object body, HttpStatus status) {
		super(body, status);
		// TODO Auto-generated constructor stub
	}



	public ResponseObject(Object body, MultiValueMap headers, HttpStatus status) {
		super(body, headers, status);
		// TODO Auto-generated constructor stub
	}



	public int getTask() {
		return task;
	}



	public void setTask(int task) {
		this.task = task;
	}



	public ResponseObject(HttpStatus status, int task) {
		super(status);
		this.task = task;
	}
	
	

}
